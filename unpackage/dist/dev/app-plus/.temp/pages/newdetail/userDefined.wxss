
.inputFrame {
  width: 100%;
  background-color: rgb(253,205,223);
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
  flex-direction: column;
}
.inputForm {
  -webkit-box-pack: center;
  -webkit-justify-content: center;
  justify-content: center;
  text-align: center;
  font-size: 36rpx;
  color: #000000;
  font-weight: 1000;
  width: 680rpx;
  padding: 20rpx;
  padding-right: 60rpx;
  margin: 20rpx 15rpx;
  background-color: rgb(243,165,183);
}
.placeHolder {
  font-size: 36rpx;
  font-weight: 1000;
  text-align: center;
}
.time-picker {
  width: 750rpx;
}
.time-picker-view {
  font-size: 36rpx;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
  justify-content: center;
