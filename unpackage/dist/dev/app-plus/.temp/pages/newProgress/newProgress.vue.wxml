<template name="6ad50cfe">
  <view class="_view data-v-18bfb90f newProgress">
    <view class="_view data-v-18bfb90f nav-bar-userset">
      <image src="/static/back-arrow.png" class="_image data-v-18bfb90f nav-bar-userset-back" bindtap="handleProxy" data-eventid="{{'x9N-0'}}" data-comkey="{{$k}}"></image>
      <image src="/static/sub-logo.png" class="_image data-v-18bfb90f nav-bar-userset-sublogo"></image>
      <view class="_view data-v-18bfb90f nav-bar-userset-add"></view>
    </view>
    <view class="_view data-v-18bfb90f inputTitle">
      <input class="_input data-v-18bfb90f inputForm" placeholder="输入TP名称" placeholder-class="placeHolder" maxlength="6" confirm-type="backToIndex" />
    </view>
    <view class="_view data-v-18bfb90f inputProgress">
      <input class="_input data-v-18bfb90f start" value="{{pro.start}}" bindinput="handleProxy" type="number" placeholder="开始" placeholder-class="placeHolder" data-eventid="{{'awW-1'}}" data-comkey="{{$k}}" />
      <input class="_input data-v-18bfb90f now" value="{{pro.now}}" bindinput="handleProxy" type="number" placeholder="现在" placeholder-class="placeHolder" data-eventid="{{'QJ5-2'}}" data-comkey="{{$k}}" />
      <input class="_input data-v-18bfb90f end" value="{{pro.end}}" bindinput="handleProxy" type="number" placeholder="结束" placeholder-class="placeHolder" data-eventid="{{'1aD-3'}}" data-comkey="{{$k}}" />
    </view>
    <view class="_view data-v-18bfb90f inputUnit">
      <input class="_input data-v-18bfb90f unit" placeholder="单位" placeholder-class="placeHolder" />
    </view>
    <view class="_view data-v-18bfb90f editProgress">
      <button class="_button data-v-18bfb90f editProgressButton" wx:if="{{pro.end>pro.start}}" bindtap="handleProxy" data-eventid="{{'Ghu-4'}}" data-comkey="{{$k}}">打卡</button>
      <button class="_button data-v-18bfb90f editProgressButton" wx:if="{{pro.end<pro.start}}" bindtap="handleProxy" data-eventid="{{'4xV-5'}}" data-comkey="{{$k}}">打卡</button>
    </view>
    <view class="_view data-v-18bfb90f addNotes">
      <view class="_view data-v-18bfb90f addButton" wx:if="{{!showNote}}" bindtap="handleProxy" data-eventid="{{'mSc-6'}}" data-comkey="{{$k}}"> 显示备注 </view>
      <view class="_view data-v-18bfb90f addButton" wx:if="{{showNote}}" bindtap="handleProxy" data-eventid="{{'esR-7'}}" data-comkey="{{$k}}"> 收起备注 </view>
      <view class="_view data-v-18bfb90f">
        <textarea class="_textarea data-v-18bfb90f Note" wx:if="{{showNote}}" placeholder="此处输入备注" />
      </view>
    </view>
    <view class="_view data-v-18bfb90f confirm">
      <button class="_button data-v-18bfb90f confirmButton" bindtap="handleProxy" data-eventid="{{'Hs0-8'}}" data-comkey="{{$k}}">完成</button>
    </view>
  </view>
</template>