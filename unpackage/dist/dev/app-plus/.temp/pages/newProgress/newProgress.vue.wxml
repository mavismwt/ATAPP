<template name="0be6cff7">
  <view class="_view data-v-1a52c274 newProgress">
    <view class="_view data-v-1a52c274 nav-bar-userset">
      <image src="/static/back-arrow.png" class="_image data-v-1a52c274 nav-bar-userset-back" bindtap="handleProxy" data-eventid="{{'CRF-0'}}" data-comkey="{{$k}}"></image>
      <image src="/static/sub-logo.png" class="_image data-v-1a52c274 nav-bar-userset-sublogo"></image>
      <view class="_view data-v-1a52c274 nav-bar-userset-add"></view>
    </view>
    <view class="_view data-v-1a52c274 inputTitle">
      <input class="_input data-v-1a52c274 inputForm" placeholder="输入TP名称" placeholder-class="placeHolder" maxlength="6" confirm-type="done" />
    </view>
    <view class="_view data-v-1a52c274 inputProgress">
      <input class="_input data-v-1a52c274 start" value="{{time.start}}" bindinput="handleProxy" type="number" placeholder="开始" placeholder-class="placeHolder" data-eventid="{{'Kq8-1'}}" data-comkey="{{$k}}" />
      <input class="_input data-v-1a52c274 now" value="{{time.now}}" bindinput="handleProxy" type="number" placeholder="现在" placeholder-class="placeHolder" data-eventid="{{'LnZ-2'}}" data-comkey="{{$k}}" />
      <input class="_input data-v-1a52c274 end" value="{{time.end}}" bindinput="handleProxy" type="number" placeholder="结束" placeholder-class="placeHolder" data-eventid="{{'Jw0-3'}}" data-comkey="{{$k}}" />
    </view>
    <view class="_view data-v-1a52c274 inputUnit">
      <input class="_input data-v-1a52c274 unit" placeholder="单位" placeholder-class="placeHolder" />
    </view>
    <view class="_view data-v-1a52c274 editProgress">
      <button class="_button data-v-1a52c274 editProgressButton" wx:if="{{time.end>time.start}}" bindtap="handleProxy" data-eventid="{{'7ck-4'}}" data-comkey="{{$k}}">打卡</button>
      <button class="_button data-v-1a52c274 editProgressButton" wx:if="{{time.end<time.start}}" bindtap="handleProxy" data-eventid="{{'4Na-5'}}" data-comkey="{{$k}}">打卡</button>
    </view>
    <view class="_view data-v-1a52c274 addNotes">
      <view class="_view data-v-1a52c274 addButton" wx:if="{{!showNote}}" bindtap="handleProxy" data-eventid="{{'6ql-6'}}" data-comkey="{{$k}}"> 显示备注 </view>
      <view class="_view data-v-1a52c274 addButton" wx:if="{{showNote}}" bindtap="handleProxy" data-eventid="{{'o0f-7'}}" data-comkey="{{$k}}"> 收起备注 </view>
      <view class="_view data-v-1a52c274">
        <textarea class="_textarea data-v-1a52c274 Note" wx:if="{{showNote}}" placeholder="此处输入备注" />
      </view>
    </view>
    <view class="_view data-v-1a52c274 confirm">
      <button class="_button data-v-1a52c274 confirmButton">完成</button>
    </view>
  </view>
</template>