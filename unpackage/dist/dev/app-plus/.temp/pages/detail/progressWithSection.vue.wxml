<template name="4902289e">
  <view class="_view data-v-88e70c00 newProgressPlus">
    <view class="_view data-v-88e70c00 nav-bar-userset">
      <image src="/static/back-arrow.png" class="_image data-v-88e70c00 nav-bar-userset-back" bindtap="handleProxy" data-eventid="{{'FEj-0'}}" data-comkey="{{$k}}"></image>
      <image src="/static/sub-logo.png" class="_image data-v-88e70c00 nav-bar-userset-sublogo"></image>
      <view class="_view data-v-88e70c00 nav-bar-userset-add"></view>
    </view>
    <view class="_view data-v-88e70c00 time-process-block">
      <view class="_view data-v-88e70c00 top-line">
        <view class="_view data-v-88e70c00 title-line">
          <input class="_input data-v-88e70c00 red-point" />
          <input class="_input data-v-88e70c00 title" value="{{schedule.title}}" bindinput="handleProxy" data-eventid="{{'18p-1'}}" data-comkey="{{$k}}" />
        </view>
        <view class="_view data-v-88e70c00 time-name">{{title}}</view>
        <view class="_view data-v-88e70c00 precess-percent">{{process}}%</view>
      </view>
      <view class="_view data-v-88e70c00 all-process">
        <view class="_view data-v-88e70c00 little-person" animation="{{animationDataPerson}}">
          <view class="_view data-v-88e70c00 chat-frame">
            <view class="_view data-v-88e70c00 .chat-frame-content">{{schedule.status.toEnd}}</view>
            <image src="/static/chat-frame.png" class="_image data-v-88e70c00 chat-frame-image"></image>
          </view>
          <image src="/static/little-person.png" class="_image data-v-88e70c00 little-person-image"></image>
        </view>
        <view class="_view data-v-88e70c00 process-bar">
          <view class="_view data-v-88e70c00 blank-bar"></view>
          <view class="_view data-v-88e70c00 finish-bar" animation="{{animationDataProcess}}"></view>
        </view>
      </view>
    </view>
    <view class="_view data-v-88e70c00 sectionView">
      <view class="_view data-v-88e70c00 addASection" bindtap="handleProxy" data-eventid="{{'Shg-2'}}" data-comkey="{{$k}}">添加阶段</view>
      <view class="_view data-v-88e70c00 sectionCell" wx:key="item.index" key="{{item.index}}" bindlongpress="handleProxy" data-id="{{item.index}}" data-eventid="{{'dIe-6-'+index}}" data-comkey="{{$k}}" wx:for="{{schedule.sectionData}}" wx:for-index="index"
        wx:for-item="item">
        <input class="_input data-v-88e70c00 inputSectionName" placeholder="输入阶段名称" placeholder-style="placeHolder" confirm-type="Next" value="{{item.name}}" bindinput="handleProxy" bindkeyup="handleProxy" data-eventid="{{'m49-3-'+index}}" data-comkey="{{$k}}"
        />
        <view class="_view data-v-88e70c00 setStatus">
          <image class="_image data-v-88e70c00 showStatus1" wx:if="{{item.isFinished}}" src="../../static/wancheng-copy.png" bindtap="handleProxy" value="{{item.isFinished}}" bindchange="handleProxy" data-eventid="{{'C3h-4-'+index}}" data-comkey="{{$k}}"></image>
          <image class="_image data-v-88e70c00 showStatus0" wx:if="{{!item.isFinished}}" src="../../static/yuanhuan.png" bindtap="handleProxy" value="{{item.isFinished}}" bindchange="handleProxy" data-eventid="{{'bRB-5-'+index}}" data-comkey="{{$k}}"></image>
        </view>
      </view>
    </view>
    <view class="_view data-v-88e70c00 addNotes">
      <view class="_view data-v-88e70c00">
        <textarea class="_textarea data-v-88e70c00 Note" wx:if="{{showNote}}" value="{{schedule.note}}" bindinput="handleProxy" placeholder="此处输入备注" data-eventid="{{'4dJ-7'}}" data-comkey="{{$k}}" />
      </view>
    </view>
    <view class="_view data-v-88e70c00 deleteView">
      <view class="_view data-v-88e70c00 deleteButton" bindtap="handleProxy" data-eventid="{{'h5h-8'}}" data-comkey="{{$k}}">删除任务</view>
      <view class="_view data-v-88e70c00 bottomView"></view>
    </view>
    <view class="_view data-v-88e70c00 confirm">
      <button class="_button data-v-88e70c00 confirmButton" bindtap="handleProxy" data-eventid="{{'jhC-9'}}" data-comkey="{{$k}}">保存并退出</button>
    </view>
  </view>
</template>