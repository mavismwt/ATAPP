<template name="0a1b8544">
  <view class="_view data-v-1222eb22 newProgress">
    <view class="_view data-v-1222eb22 nav-bar-userset">
      <image src="/static/back-arrow.png" class="_image data-v-1222eb22 nav-bar-userset-back" bindtap="handleProxy" data-eventid="{{'hPm-0'}}" data-comkey="{{$k}}"></image>
      <image src="/static/sub-logo.png" class="_image data-v-1222eb22 nav-bar-userset-sublogo"></image>
      <image src="/static/tick.png" class="_image data-v-1222eb22 nav-bar-userset-tick" bindtap="handleProxy" data-eventid="{{'f1l-1'}}" data-comkey="{{$k}}"></image>
    </view>
    <view class="_view data-v-1222eb22 time-process-block">
      <view class="_view data-v-1222eb22 top-line">
        <view class="_view data-v-1222eb22 title-line">
          <input class="_input data-v-1222eb22 red-point" />
          <input class="_input data-v-1222eb22 title" value="{{schedule.title}}" bindinput="handleProxy" maxlength="6" data-eventid="{{'0zm-2'}}" data-comkey="{{$k}}" />
        </view>
        <view class="_view data-v-1222eb22 time-name">{{title}}</view>
        <view class="_view data-v-1222eb22 precess-percent">{{process}}%</view>
      </view>
      <view class="_view data-v-1222eb22 all-process">
        <view class="_view data-v-1222eb22 little-person" animation="{{animationDataPerson}}">
          <view class="_view data-v-1222eb22 chat-frame">
            <view class="_view data-v-1222eb22 .chat-frame-content">{{schedule.status.toEnd}}</view>
            <image src="../../static/chat-gray.png" class="_image data-v-1222eb22 chat-frame-image"></image>
          </view>
          <image src="/static/little-person.png" class="_image data-v-1222eb22 little-person-image"></image>
        </view>
        <view class="_view data-v-1222eb22 process-bar">
          <view class="_view data-v-1222eb22 blank-bar"></view>
          <view class="_view data-v-1222eb22 finish-bar" animation="{{animationDataProcess}}"></view>
        </view>
      </view>
    </view>
    <view class="_view data-v-1222eb22 inputProgress">
      <view class="_view data-v-1222eb22 sectionInfo">
        <view class="_view data-v-1222eb22 sectionTag">开始于</view>
        <input class="_input data-v-1222eb22 time" value="{{schedule.time.start}}" bindinput="handleProxy" type="number" placeholder="开始" placeholder-class="placeHolder" data-eventid="{{'yo1-3'}}" data-comkey="{{$k}}" />
        <view class="_view data-v-1222eb22 unitTag">{{schedule.unit}}</view>
      </view>
      <view class="_view data-v-1222eb22 sectionInfo">
        <view class="_view data-v-1222eb22 sectionTag">现处于</view>
        <input class="_input data-v-1222eb22 time" value="{{schedule.time.now}}" bindinput="handleProxy" type="number" placeholder="现在" placeholder-class="placeHolder" data-eventid="{{'3k4-4'}}" data-comkey="{{$k}}" />
        <view class="_view data-v-1222eb22 unitTag">{{schedule.unit}}</view>
      </view>
      <view class="_view data-v-1222eb22 sectionInfo">
        <view class="_view data-v-1222eb22 sectionTag">目标为</view>
        <input class="_input data-v-1222eb22 time" value="{{schedule.time.end}}" bindinput="handleProxy" type="number" placeholder="结束" placeholder-class="placeHolder" data-eventid="{{'9KB-5'}}" data-comkey="{{$k}}" />
        <view class="_view data-v-1222eb22 unitTag">{{schedule.unit}}</view>
      </view>
    </view>
    <view class="_view data-v-1222eb22 editProgress">
      <button class="_button data-v-1222eb22 editProgressButton" bindtap="handleProxy" data-eventid="{{'qBS-6'}}" data-comkey="{{$k}}"> 打卡 </button>
    </view>
    <view class="_view data-v-1222eb22 addNotes">
      <view class="_view data-v-1222eb22">
        <textarea class="_textarea data-v-1222eb22 Note" wx:if="{{showNote}}" placeholder="此处输入备注" value="{{schedule.note}}" bindinput="handleProxy" data-eventid="{{'yn6-7'}}" data-comkey="{{$k}}" />
      </view>
    </view>
    <view class="_view data-v-1222eb22 deleteView">
      <button class="_button data-v-1222eb22 deleteButton" bindtap="handleProxy" data-eventid="{{'jsw-8'}}" data-comkey="{{$k}}">删除任务</button>
      <view class="_view data-v-1222eb22 bottomView"></view>
    </view>
  </view>
</template>